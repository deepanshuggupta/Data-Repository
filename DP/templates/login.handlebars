{{!-- The template for showing all quizzes --}}
<style>
	#divLoginTemplate {
	}
</style>
<div id="divLoginTemplate">
	<form class="form-horizontal">
	    <div class="form-group">
	        <label for="txtUser" class="col-sm-2 control-label">User</label>
	        <div class="col-sm-10">
	            <input type="text" class="form-control" id="txtUser" placeholder="User">
	        </div>
	    </div>
	    <div class="form-group">
	        <label for="txtPwd" class="col-sm-2 control-label">Password</label>
	        <div class="col-sm-10">
	            <input type="password" class="form-control" id="txtPwd" placeholder="Password">
	        </div>
	    </div>
	    <div class="form-group">
	        <div class="col-sm-offset-2 col-sm-10">
	            <button id="btnLogin" type="submit" class="btn btn-default">Sign in</button>
	        </div>
	    </div>
	</form>
</div>


